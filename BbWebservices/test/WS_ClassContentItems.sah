<browser>
function setTMEContent(fieldName, content) {
var wp = top.document.getElementById('contentFrame').contentWindow.tinyMceWrapper;
var edt = wp.getEditor(fieldName);
var tedt = edt._tinyMceEditor;
tedt.setContent(content);
}
